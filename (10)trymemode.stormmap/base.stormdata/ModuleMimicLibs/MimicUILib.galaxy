    }

    MimicUILib_InitLib_completed = true;

    MimicUILib_InitLibraries();
    MimicUILib_InitVariables();
    MimicUILib_InitTriggers();
	UIDisplayMessage(PlayerGroupAll(), c_messageAreaDebug, StringToText("UILib Mimic Lib Initialized"));
}

