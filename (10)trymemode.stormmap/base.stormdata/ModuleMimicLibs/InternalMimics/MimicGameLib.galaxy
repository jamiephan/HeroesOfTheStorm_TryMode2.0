//--------------------------------------------------------------------------------------------------
bool MimicGameLib_gt_PlayerLeavesGame_Func (bool testConds, bool runActions) {
    // Variable Declarations
    int lv_itPlayer;
    int lv_team;
    int lv_enemyTeam;

    // Automatic Variable Declarations
    // Variable Initialization

    // Conditions
    if (testConds) {
        if (!((MimicGameLib_gv_players[EventPlayer()].lv_gameEndedForPlayer == false))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    if ((MimicGameLib_gv_gameOver == false)) {
        TimerStart(MimicGameLib_gv_players[EventPlayer()].lv_playerLeftAnnouncementTimer, MimicGameLib_gv_playerLeftAnnouncementTimerDuration_C, false, c_timeGame);
        MimicGameLib_gv_players[EventPlayer()].lv_playerStatus = MimicGameLib_ge_PlayerStatusStorm_LefttheGame;
        lv_team = MimicGameLib_gf_TeamNumberOfPlayer(EventPlayer());
        MimicGameLib_gv_teams[lv_team].lv_activeHumanPlayers -= 1;
        if ((MimicGameLib_gv_teams[lv_team].lv_activeHumanPlayers == 0)) {
            lv_enemyTeam = (3 - lv_team);
            TextExpressionSetToken("Param/Expression/lib_Game_2995AAD8", "A", FixedToText(MimicGameLib_gv_teamAbandonedTheGameTimerDuration_C, 0));
            UIDisplayMessage(MimicGameLib_gv_teams[lv_enemyTeam].lv_teamPlayerGroupNoNPCPlayers, c_messageAreaChat, TextExpressionAssemble("Param/Expression/lib_Game_2995AAD8"));
            TimerStart(MimicGameLib_gv_teams[lv_team].lv_abandonedTheGameTimer, MimicGameLib_gv_teamAbandonedTheGameTimerDuration_C, false, c_timeGame);
        }

    }

    return true;
}

