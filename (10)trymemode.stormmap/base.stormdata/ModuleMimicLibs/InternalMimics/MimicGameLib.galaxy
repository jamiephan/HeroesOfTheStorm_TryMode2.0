//--------------------------------------------------------------------------------------------------
bool MimicGameLib_gt_InitializeWatchTowers_Func (bool testConds, bool runActions) {
    // Variable Declarations
    unit lv_u;
    int lv_player;
    int lv_i;

    // Automatic Variable Declarations
    unitgroup autoDB8A231F_g;
    int autoDB8A231F_u;

    // Variable Initialization

    // Conditions
    if (testConds) {
        if (!((libCore_gv_sYSJungleWatchTowers == true))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    autoDB8A231F_g = UnitGroup("XelNagaWatchTower", c_playerAny, RegionEntireMap(), UnitFilter(0, 0, (1 << c_targetFilterMissile), (1 << (c_targetFilterDead - 32)) | (1 << (c_targetFilterHidden - 32))), 0);
    autoDB8A231F_u = UnitGroupCount(autoDB8A231F_g, c_unitCountAll);
    for (;; autoDB8A231F_u -= 1) {
        lv_u = UnitGroupUnitFromEnd(autoDB8A231F_g, autoDB8A231F_u);
        if (lv_u == null) { break; }
        MimicGameLib_gv_watchTowersCurrentCount += 1;
        MimicGameLib_gv_watchTowers[MimicGameLib_gv_watchTowersCurrentCount].lv_watchTowerUnit = lv_u;
        UnitSetCustomValue(lv_u, libCore_gf_CustomValueIndex(libCore_ge_CustomValueIndexes_WatchTowerIndex), MimicGameLib_gv_watchTowersCurrentCount);
        libNtve_gf_CreateUnitsWithDefaultFacing(1, "WatchTowerCaptureBeacon", c_unitCreateIgnorePlacement, UnitGetOwner(lv_u), UnitGetPosition(lv_u), null);
        MimicGameLib_gf_CapturePointCreate(UnitLastCreated(), libCore_gv_data_WatchTower_CaptureRadius_C, libCore_gv_data_WatchTower_CaptureTime_C, 0.0, MimicGameLib_ge_CapturePointDecay_DoNotDecay, MimicGameLib_ge_CapturePointDecay_Owner, true);
        MimicGameLib_gv_watchTowers[MimicGameLib_gv_watchTowersCurrentCount].lv_capturePointIndex = MimicGameLib_gf_LastCreatedCapturePoint();
        AddUnitOfInterest(MimicGameLib_gv_watchTowers[MimicGameLib_gv_watchTowersCurrentCount].lv_watchTowerUnit, 3.5, 0.0, "VisionBeacon", libNtve_ge_GoalTeam_Both, MimicGameLib_gv_watchTowers[MimicGameLib_gv_watchTowersCurrentCount].lv_capturePointIndex);
    }
    TriggerEnable(MimicGameLib_gt_WatchTowersOwnershipChanges, true);
    return true;
}

