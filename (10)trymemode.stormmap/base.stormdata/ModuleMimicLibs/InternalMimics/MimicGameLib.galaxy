void MimicGameLib_gf_HeroDeathUpdateRespawnTimer (int lp_respawningPlayer, fixed lp_newRespawnTimerDuration) {
    // Variable Declarations
    fixed lv_currentRespawnTimer;
    fixed lv_soundOffset;

    // Automatic Variable Declarations
    // Variable Initialization

    // Implementation
    lv_currentRespawnTimer = TimerGetRemaining(MimicGameLib_gv_players[lp_respawningPlayer].lv_respawnTimer);
    if ((lv_currentRespawnTimer == 0.0)) {
        return ;
    }

    if ((lv_currentRespawnTimer < lp_newRespawnTimerDuration)) {
        UnitBehaviorSetDuration(MimicGameLib_gv_players[lp_respawningPlayer].lv_heroUnit, "Dead", lp_newRespawnTimerDuration);
    }
    else {
        UnitBehaviorSetDurationRemaining(MimicGameLib_gv_players[lp_respawningPlayer].lv_heroUnit, "Dead", lp_newRespawnTimerDuration);
    }
    TimerStart(MimicGameLib_gv_players[lp_respawningPlayer].lv_respawnTimer, lp_newRespawnTimerDuration, false, c_timeGame);
    TimerStart(MimicGameLib_gv_players[lp_respawningPlayer].lv_respawnSoundCueTimer, MaxF((lp_newRespawnTimerDuration - MimicGameLib_gv_heroDeathRespawnCueSoundTiming), 0.0625), false, c_timeGame);
